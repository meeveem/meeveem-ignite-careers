{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/((?!assets|.*\\..*).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
    ,
    {
      "source": "/(favicon.ico|favicon-16x16.png|favicon-32x32.png|favicon-48x48.png|favicon-64x64.png|favicon-128x128.png|android-chrome-192x192.png|android-chrome-512x512.png|apple-touch-icon.png|site.webmanifest)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
  ,
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "meeveem.com" }],
      "destination": "https://www.meeveem.com/:path*",
      "permanent": true
    }
  ]
}
